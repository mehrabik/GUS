    <!-- Defining Policies -->
    <Policies>
          <!-- Deny sensitve apps from starting dangerous activities when the user is secondary (no matter the authentication confidence) -->
          <Policy ActionID="0" DomainID="2" TypeID="203" OperationID="2" RoleID="2" ModalityID="0" Conf="0"/>

          <!-- Deny sensitve apps from starting dangerous activities when the user is primary but authenticated with less than 99% confidence -->
          <Policy ActionID="0" DomainID="2" TypeID="203" OperationID="2" RoleID="3" ModalityID="0" Conf="-99"/>

          <!-- Deny normal apps from starting dangerous activities when the user is secondary (no matter the confidence) -->
          <Policy ActionID="0" DomainID="1" TypeID="203" OperationID="2" RoleID="2" ModalityID="0" Conf="0"/>

          <!-- Deny normal apps from starting dangerous activities when the user is primary but authenticated with less than 99% confidence -->
          <Policy ActionID="0" DomainID="1" TypeID="203" OperationID="2" RoleID="3" ModalityID="0" Conf="-99"/>

          <!-- Deny normal apps from starting sensitive or dangerous activities when the user is guest (confidence is irrelevant here) -->
          <Policy ActionID="0" DomainID="1" TypeID="202" OperationID="2" RoleID="1" ModalityID="0" Conf="0"/>
          <Policy ActionID="0" DomainID="1" TypeID="203" OperationID="2" RoleID="1" ModalityID="0" Conf="0"/>

          <!-- Deny access to dangerous apps for guest and secondary users (no matter the confidence) -->
          <Policy ActionID="0" DomainID="3" TypeID="0" OperationID="2" RoleID="2" ModalityID="0" Conf="0"/>
          <Policy ActionID="0" DomainID="3" TypeID="0" OperationID="2" RoleID="1" ModalityID="0" Conf="0"/>

          <!-- Deny access to dangerous apps for primary user when confidence is below 99% -->
          <Policy ActionID="0" DomainID="3" TypeID="0" OperationID="2" RoleID="3" ModalityID="0" Conf="-99"/>

          <!-- Deny access to sensitive apps for primary user when confidence is below 50% -->
          <Policy ActionID="0" DomainID="2" TypeID="0" OperationID="2" RoleID="3" ModalityID="0" Conf="-50"/>

          <!-- Deny access to sensitive apps for secondary user when confidence is below 99% -->
          <Policy ActionID="0" DomainID="2" TypeID="0" OperationID="2" RoleID="2" ModalityID="0" Conf="-99"/>

          <!-- Deny access to sensitive apps for guest (no matter the confidence) -->
          <Policy ActionID="0" DomainID="2" TypeID="0" OperationID="2" RoleID="1" ModalityID="0" Conf="0"/>
    </Policies>
