    <!-- Defining Policies -->
    <Policies>
          <!-- Deny sensitve apps to request dangerous permissions when user is secondary (no matter the authentication confidence) -->
          <Policy ActionID="0" DomainID="2" TypeID="303" OperationID="1" RoleID="2" ModalityID="0" Conf="0"/>

          <!-- Deny sensitve apps to request dangerous permissions when user is primary but authenticated with less than 99% confidence -->
          <Policy ActionID="0" DomainID="2" TypeID="303" OperationID="1" RoleID="3" ModalityID="0" Conf="-0.99"/>

          <!-- Deny normal apps to request dangerous permissions when user is secondary (no matter the authentication confidence) -->
          <Policy ActionID="0" DomainID="1" TypeID="303" OperationID="1" RoleID="2" ModalityID="0" Conf="0"/>

          <!-- Deny normal apps to request dangerous permissions when user is primary but authenticated with less than 99% confidence  -->
          <Policy ActionID="0" DomainID="1" TypeID="303" OperationID="1" RoleID="3" ModalityID="0" Conf="-0.99"/>

          <!-- Deny normal apps to request sensitive or dangerous permissions when user is guest -->
          <Policy ActionID="0" DomainID="1" TypeID="302" OperationID="1" RoleID="1" ModalityID="0" Conf="0"/>
          <Policy ActionID="0" DomainID="1" TypeID="303" OperationID="1" RoleID="1" ModalityID="0" Conf="0"/>

          <!-- Deny access to dangerous apps for guest and secondary -->
          <Policy ActionID="0" DomainID="3" TypeID="0" OperationID="1" RoleID="2" ModalityID="0" Conf="0"/>
          <Policy ActionID="0" DomainID="3" TypeID="0" OperationID="1" RoleID="1" ModalityID="0" Conf="0"/>

          <!-- Deny access to sensitive apps when user is secondary but authenticated with less than 99% confidence -->
          <Policy ActionID="0" DomainID="2" TypeID="0" OperationID="1" RoleID="2" ModalityID="0" Conf="-0.99"/>

          <!-- Deny access to sensitive apps when user is primary but authenticated with less than 50% confidence -->
          <Policy ActionID="0" DomainID="2" TypeID="0" OperationID="1" RoleID="2" ModalityID="0" Conf="-0.50"/>

          <!-- Deny access to sensitive apps for guest -->
          <Policy ActionID="0" DomainID="2" TypeID="0" OperationID="1" RoleID="1" ModalityID="0" Conf="0"/>
    </Policies>
